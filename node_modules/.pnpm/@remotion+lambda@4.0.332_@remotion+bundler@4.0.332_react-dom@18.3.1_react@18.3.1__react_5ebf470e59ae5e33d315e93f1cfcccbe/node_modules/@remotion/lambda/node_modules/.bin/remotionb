#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/s1/build/director-video-editor-microfrontend/node_modules/.pnpm/@remotion+cli@4.0.332_react-dom@18.3.1_react@18.3.1__react@18.3.1/node_modules/@remotion/cli/node_modules:/Users/s1/build/director-video-editor-microfrontend/node_modules/.pnpm/@remotion+cli@4.0.332_react-dom@18.3.1_react@18.3.1__react@18.3.1/node_modules/@remotion/node_modules:/Users/s1/build/director-video-editor-microfrontend/node_modules/.pnpm/@remotion+cli@4.0.332_react-dom@18.3.1_react@18.3.1__react@18.3.1/node_modules:/Users/s1/build/director-video-editor-microfrontend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/s1/build/director-video-editor-microfrontend/node_modules/.pnpm/@remotion+cli@4.0.332_react-dom@18.3.1_react@18.3.1__react@18.3.1/node_modules/@remotion/cli/node_modules:/Users/s1/build/director-video-editor-microfrontend/node_modules/.pnpm/@remotion+cli@4.0.332_react-dom@18.3.1_react@18.3.1__react@18.3.1/node_modules/@remotion/node_modules:/Users/s1/build/director-video-editor-microfrontend/node_modules/.pnpm/@remotion+cli@4.0.332_react-dom@18.3.1_react@18.3.1__react@18.3.1/node_modules:/Users/s1/build/director-video-editor-microfrontend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/bun" ]; then
  exec "$basedir/bun"  "$basedir/../../../../../../@remotion+cli@4.0.332_react-dom@18.3.1_react@18.3.1__react@18.3.1/node_modules/@remotion/cli/remotionb-cli.js" "$@"
else
  exec bun  "$basedir/../../../../../../@remotion+cli@4.0.332_react-dom@18.3.1_react@18.3.1__react@18.3.1/node_modules/@remotion/cli/remotionb-cli.js" "$@"
fi
